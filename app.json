            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptive-icon.png",
                "backgroundColor": "#FFFFFF",
                "package": "com.phong.shopping",
                "versionCode": 1
            }
        },
        "web": {
            "favicon": "./assets/favicon.png"
        },
        "description": "",
        "githubUrl": "https://github.com/Mobile-Development-HCMUE/shopping-cart"
    }
}
